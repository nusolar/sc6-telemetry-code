{
	"manifest_version": 2,
	"name": "NUSolar ZELDA System",
	"version": "0.0.1",
	"minimum_chrome_version": "27",
	"icons": {
		"256": "icon_256.png"
	},
	"app": {
		"background": {
			"scripts": ["jquery-2.0.3.js", "angular.min.js", "background.js"]
		}
	},
	"sandbox": {
		"pages": ["map_viewer.html"]
	},
	"permissions": [
		"serial",
		"tts",
		"clipboardRead",
		"clipboardWrite",
		"fullscreen"
	]
}
